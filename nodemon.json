{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/*",
        "public"
    ],
    "delay": "2500",
    "verbose": true,
    "execMap": {
        "js": "node --harmony --async-stack-traces --harmony-await-optimization --experimental-modules --inspect=0.0.0.0:9229"
    },
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js,json"
    }